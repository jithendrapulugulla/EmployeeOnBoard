# Staging Environment Variables
NODE_ENV=staging

# MongoDB Atlas Connection String
MONGODB_URI=${MONGODB_URI_STAGING}

# Port
PORT=8080

# JWT Secret
JWT_SECRET=${JWT_SECRET_STAGING}

# Email Configuration
EMAIL_USER=${EMAIL_USER_STAGING}
EMAIL_PASSWORD=${EMAIL_PASSWORD_STAGING}

# Frontend URL
FRONTEND_URL=https://staging-app-name.azurewebsites.net

# Application Settings
LOG_LEVEL=info
